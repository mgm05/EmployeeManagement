<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.DogTypeMapper">
	<resultMap type="com.example.demo.entity.dog.DogTypeEntity" id="DogTypeMap">
		<result column="DOG_ID" jdbcType="INTEGER" property="dogTypeId"/>
		<result column="DOG_TYPE_NM" jdbcType="VARCHAR" property="dogTypeName"/>
	</resultMap>
	<select id="selectDogType" resultMap="DogTypeMap">
		SELECT dog_type_code , dog_type_nm FROM dog_type
	</select>
</mapper>